fn main(){
    //Criando uma tupla com diferentes tipos de dados
    let pessoa = ("Ana", 28, 1.65, true);
    
    //Acessando os elementos da tupla
    let nome = pessoa.0;
    let idade = pessoa.1;
    let altura = pessoa.2;
    let ativo = pessoa.3;
    
    //Exibindo as informações
    println!("Nome: {}",nome);
    println!("Idade: {}",idade);
    println!("Altura: {}",altura);
    println!("Esta ativo? {}",ativo);
}
